            "common": {
                "name": "Data"
            }
        },
        {
            "_id": "data.json",
            "type": "state",
            "common": {
                "name": "JSON",
                "desc": "JSON object with next 36 hours",
                "type": "string",
                "role": "json",
                "read": true,
                "write": false,
                "def": "[]"
            },
            "native": {
            }
        },
        {
            "_id": "data.start",
            "type": "state",
            "common": {
                "name": "Next green period",
                "desc": "Start of the current or next green period",
                "type": "string",
                "role": "date",
                "read": true,
                "write": false
            },
            "native": {
            }
        },
        {
            "_id": "data.duration",
            "type": "state",
            "common": {
                "name": "Next green period",
                "desc": "Duration in hours of the current or next green period",
                "type": "number",
                "unit": "hours",
                "role": "value.duration",
                "read": true,
                "write": false
            },
            "native": {
            }
        },
        {
            "_id": "data.green",
            "type": "state",
            "common": {
                "name": "Green period",
                "desc": "Is now green period",
                "type": "boolean",
                "role": "state",
                "read": true,
                "write": false
            },
            "native": {
            }
        },
        {
            "_id": "data.price",
            "type": "state",
            "common": {
                "name": "price",
                "desc": "Current energy price",
                "type": "number",
                "role": "state",
                "unit": "€",
                "read": true,
                "write": false
